msgctxt "@info:whatsthis"
msgid   "<shortcut>Click</shortcut> on a track view in the project monitor to "
        "perform a lift of all tracks except active one"
msgstr  "Feu <shortcut>clic</shortcut> sobre una vista de pista a la pantalla "
        "del projecte per a realisar una elevació de totes les pistes, "
        "excepte de l'activa"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Click</shortcut> on an item to slip, <shortcut>Shift "
        "click</shortcut> for multiple selection"
msgstr  "Feu <shortcut>clic</shortcut> sobre un element per a lliscar, "
        "<shortcut>Maj + clic</shortcut> per a una selecció múltiple"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Click</shortcut> to add points, <shortcut>Right click</"
        "shortcut> to close shape."
msgstr  "Feu <shortcut>clic</shortcut> per a afegir punts, feu <shortcut>clic "
        "dret</shortcut> per a tancar la forma."

msgctxt "@info:whatsthis"
msgid   "<shortcut>Click</shortcut> to play, <shortcut>Double click</"
        "shortcut> for fullscreen, <shortcut>Hover right</shortcut> for "
        "toolbar, <shortcut>Wheel</shortcut> or <shortcut>arrows</shortcut> "
        "to seek, <shortcut>Ctrl wheel</shortcut> to zoom"
msgstr  "Feu <shortcut>clic</shortcut> per a reproduir, <shortcut>doble clic</"
        "shortcut> per a pantalla completa, <shortcut>aneu a la dreta</"
        "shortcut> per a la barra d'eines, utiliseu la <shortcut>roda del "
        "ratolí</shortcut> o les <shortcut>fletxes del teclat</shortcut> per "
        "a buscar, utiliseu la <shortcut>roda + Ctrl</shortcut> per a fer zoom"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Ctrl</shortcut> to apply on current track only, "
        "<shortcut>Shift</shortcut> to also move guides. You can combine both "
        "modifiers."
msgstr  "<shortcut>Ctrl</shortcut> per a només aplicar sobre la pista actual, "
        "<shortcut>Maj</shortcut> per a també moure les guies. Podeu combinar "
        "ambdós modificadors."

msgctxt "@info:whatsthis"
msgid   "<shortcut>Double click</shortcut> on center to resize, "
        "<shortcut>Double click</shortcut> on line segment to add new point, "
        "<shortcut>Double click</shortcut> point to delete it, "
        "<shortcut>Double click</shortcut> background to create new keyframe, "
        "<shortcut>Hover right</shortcut> for toolbar"
msgstr  "Feu <shortcut>doble clic</shortcut> sobre el centre per a canviar la "
        "mida, feu <shortcut>doble clic</shortcut> sobre un segment de llínea "
        "per a afegir un punt nou, feu <shortcut>doble clic</shortcut> sobre "
        "un punt per a suprimir-lo, feu <shortcut>doble clic</shortcut> sobre "
        "el fons per a crear un fotograma clau nou, <shortcut>aneu a la "
        "dreta</shortcut> per a la barra d'eines"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Double click</shortcut> to add a keyframe, <shortcut>Shift "
        "drag</shortcut> for proportional rescale, <shortcut>Ctrl drag</"
        "shortcut> for center-based rescale, <shortcut>Hover right</shortcut> "
        "for toolbar, <shortcut>Click</shortcut> on a center to seek to its "
        "keyframe"
msgstr  "Feu <shortcut>doble clic</shortcut> per a afegir un fotograma clau, "
        "<shortcut>Maj + arrossegar</shortcut> per a convertir a una escala "
        "proporcional, <shortcut>Ctrl + arrossegar</shortcut>per a convertir "
        "a una escala basada en el centre, <shortcut>aneu a la dreta</"
        "shortcut> per a la barra d'eines, feu <shortcut>clic</shortcut> "
        "sobre un centre per a buscar el seu fotograma clau"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Shift drag handle</shortcut> for center-based resize"
msgstr  "<shortcut>Arrossegueu l'ansa</shortcut> per a redimensionar basant-"
        "se en el centre"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Shift drag</shortcut> for rubber-band selection, "
        "<shortcut>Shift click</shortcut> for multiple selection, "
        "<shortcut>Ctrl drag</shortcut> to pan"
msgstr  "<shortcut>Maj + arrossegar</shortcut> per a una selecció de banda "
        "elàstica, <shortcut>Maj + clic</shortcut> per a una selecció "
        "múltiple, <shortcut>Ctrl + arrossegar</shortcut> per a una panoràmica"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Shift</shortcut> to preview cut frame"
msgstr  "<shortcut>Maj</shortcut> per a previsualitzar el fotograma retallat"

msgctxt "@info:whatsthis"
msgid   "<shortcut>Wheel</shortcut> or <shortcut>arrows</shortcut> to seek 1 "
        "frame, <shortcut>Shift</shortcut> to seek 1 second, <shortcut>Alt</"
        "shortcut> to seek to marker, <shortcut>Home</shortcut> / "
        "<shortcut>End</shortcut> to go to first / last frame"
msgstr  "Utiliseu la <shortcut>roda del ratolí</shortcut> o les "
        "<shortcut>fletxes del teclat</shortcut> per a buscar 1 fotograma, "
        "<shortcut>Maj</shortcut> per a buscar 1 segon, <shortcut>Alt</"
        "shortcut> per a buscar el marcador, <shortcut>Inici</shortcut> / "
        "<shortcut>Fi</shortcut> per a anar al primer / últim fotograma"

